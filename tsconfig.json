{
    "compilerOptions": {
      "baseUrl": ".",
      "rootDir": "./",
      "outDir": "dist/tsc/",
      "noEmitOnError": true,
      "sourceMap": true,
      "module": "amd",
      "target": "es5",
      "lib": [ "es2015", "dom" ],
      "types": [ "mendix-client", "jasmine", "core-js" ],
      "moduleResolution": "node",
      "declaration": false,
      "removeComments": true,
      "noLib": false,
      "watch": false,
      "forceConsistentCasingInFileNames": true,
      "noFallthroughCasesInSwitch": true,
      "strict": true,
      "alwaysStrict": true,
      "skipLibCheck": false,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "strictBindCallApply": true
    },
    "exclude": [
      "dist/",
      "node_modules/"
    ]
}
